{
  "name": "react-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "parcel serve public/index.html",
    "build": "rm -rf build/*; parcel build public/index.html --dist-dir build",
    "surge": "rm -rf build/*; parcel build public/index.html --dist-dir build; echo fragile-calendar.surge.sh > build/CNAME; surge build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "parcel": "^2.0.0-nightly.792",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.2.7"
  }
}
