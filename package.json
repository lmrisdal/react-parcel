{
  "name": "react-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "prebuild": "run-s clean",
    "clean": "rimraf ./dist",
    "build": "parcel build public/index.html --dist-dir dist",
    "surge": "rimraf ./dist; parcel build public/index.html --dist-dir dist; echo fragile-calendar.surge.sh > dist/CNAME; surge dist",
    "lint": "run-s lint:code",
    "lint:code": "eslint --ext ts,tsx src",
    "start": "parcel serve public/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.792",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-modules": "^4.2.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  }
}
